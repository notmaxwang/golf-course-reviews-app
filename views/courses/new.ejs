<%- include('../partials/header')%>

<form id="new-form" class="mt-4" action="/courses" method="POST">
    <div class="form-group">
        <label>Golf Course Name:</label>
        <textarea class="form-control" type="text" name="name"></textarea><br>

        <label>Location:</label>
        <textarea class="form-control" type="text" name="location"></textarea><br>

        <label>Price:</label>
        <input type="text" name="price">

        <button type="submit" class="btn btn-outline-dark">Add Course</button>
    </div>
</form>

<%- include('../partials/footer')%>